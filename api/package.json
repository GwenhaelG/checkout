{
  "name": "api-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@slack/web-api": "^6.0.0",
    "analytics-node": "^3.5.0",
    "aws-sdk": "^2.718.0",
    "body-parser": "^1.19.0",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dompurify": "^2.2.7",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^5.3.3",
    "express-jwt-authz": "^2.4.1",
    "express-winston": "^4.0.3",
    "jest": "^27.4.7",
    "jsdom": "^16.5.1",
    "jwks-rsa": "^1.12.2",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.26.0",
    "node-fetch": "^2.6.6",
    "node-geometry-library": "^1.2.3",
    "node-mailjet": "^3.3.1",
    "node-zendesk": "^2.0.3",
    "object-hash": "^2.0.3",
    "pg": "^8.6.0",
    "sequelize": "^6.2.4",
    "stripe": "^8.89.0",
    "supertest": "^6.2.1",
    "winston": "^3.3.3",
    "winston-loggly-bulk": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sequelize-cli": "^6.1.0"
  },
  "scripts": {
    "test:nodb": "jest",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "start": "nodemon ./src/server.js",
    "deploy:staging": "eb deploy api-template-staging",
    "deploy:prod": "eb deploy api-template-prod",
    "dev": "nodemon ./src/server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --name $npm_config_name",
    "db:reset": "sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:migrate:test": "NODE_ENV=test npm run db:migrate",
    "db:migrate:staging": "NODE_ENV=staging npm run db:migrate",
    "db:reset:staging": "NODE_ENV=staging npm run db:reset",
    "db:migrate:prod": "NODE_ENV=production npm run db:migrate"
  }
}
